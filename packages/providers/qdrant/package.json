{
  "name": "@khaveeai/provider-qdrant",
  "version": "0.1.0",
  "description": "Qdrant vector database provider for Khavee AI SDK",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "qdrant",
    "vector-database",
    "embedding",
    "search",
    "khavee-ai",
    "typescript"
  ],
  "author": "Khavee AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/khaveeai/khavee-ai-sdk.git",
    "directory": "packages/providers/qdrant"
  },
  "peerDependencies": {
    "@khaveeai/core": "^0.1.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.9.0",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}